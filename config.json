{
    "actions": [
        {
            "name": "List",
            "desc": "List directory",
            "list": [
                {
                    "cmd": "ls",
                    "args": [
                        "-lta"
                    ],
                    "outFilter": "gtool",
                    "outFile" : "memory:listDir",
                    "delay": 1000
                },
                {
                    "cmd": "echo",
                    "args": [
                        "DONE Playing arround"
                    ]
                }
            ]
        },
        {
            "name": "Get Back",
            "desc": "Result from list dir",
            "list": [
                {
                    "cmd": "cat",
                    "args": [],
                    "in": "file:confOut.txt|stu,=,1,,b\n"
                }
            ]
        },
        {
            "name": "Get",
            "desc": "db.txt stuartdd",
            "list": [
                {
                    "cmd": "wget",
                    "outFile":"memory:stuartdd",
                    "args": [
                        "-q",
                        "--output-document=-",
                        "http://192.168.1.243:8080/files/user/stuart/loc/mydb/name/stuartdd"
                    ]
                },
                {
                    "cmd": "echo",
                    "args": [
                        "DONE Playing arround [%{stuartdd}] "
                    ]
                }
            ]
        },
        {
            "name": "Get",
            "desc": "db.txt stuartdd2",
            "list": [
                {
                    "cmd": "wget",
                    "outFile":"memory:stuartdd2",
                    "args": [
                        "-q",
                        "--output-document=-",
                        "http://192.168.1.243:8080/files/user/stuart/loc/mydb/name/stuartdd2"
                    ]
                },
                {
                    "cmd": "echo",
                    "outFile":"gitpushsdd2.sh",
                    "args": [
                        "gitpush.sh stuartdd %{stuartdd2}"
                    ]
                },
                {
                    "cmd": "chmod",
                    "args": [
                        "+x","gitpushsdd2.sh"
                    ]
                }
            ]
        }
    ]
}